        function BindAppRefData(ctrl, parentId) {
            //
            $.ajax({
                method: "GET",
                url: "/ProjectApi/GetAppRefData?parentId=" + parentId,
                success: function (data) {
                   
                    $(ctrl).empty();
                    $(data).each(function () {
                        $(ctrl).append($("<option></option>").val(this.keyId).html(this.keyName));
                    });
                },
                error: function () { }
            });
        }







